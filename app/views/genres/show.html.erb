<h1><%= @genre.name %></h1>
<h2>Songs</h2>
<ul>
  <% @genre.songs.each do |song| %>
    <li>
      <%= link_to song.name, song_path(song) %><br>
      Artist: <%= link_to song.artist.name, artist_path(song.artist) %>
      <br><br>
    </li>
  <% end %>
</ul>
<hr>
<%= link_to "Edit", edit_genre_path(@genre) %>
